{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Kepasa Africa",
    "keywords": [
        "Town",
        "Promoted town",
        "Ngoma digitech",
        "scott tresor",
        "South Africa",
        "Events",
        "Event Town",
        "town Event"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "fruitcake/laravel-cors": "^2.2",
        "laravel/framework": "^v9.52.9",
        "laravel/sanctum": "^2.15.1",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.8.1",
        "laravel/ui": "^3.4",
        "milon/barcode": "^9.0",
        "realrashid/sweet-alert": "^5.1.0",
        "silviolleite/laravelpwa": "^2.0",
        "stevebauman/location": "^6.6.2",
        "symfony/filesystem": "^6.3.0",
        "zepson/dpo-laravel": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8.1",
        "barryvdh/laravel-ide-helper": "^2.13.0",
        "spatie/laravel-ignition": "^1.6.4",
        "fakerphp/faker": "^1.23.0",
        "laravel/pint": "^1.10.2",
        "laravel/sail": "^1.22.0",
        "nunomaduro/phpinsights": "^2.8",
        "phpunit/phpunit": "^9.6.9",
        "mockery/mockery": "^1.6.2",
        "nunomaduro/collision": "^6.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "insight": [
            "./vendor/bin/phpinsights --fix"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
              ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
